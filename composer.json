{
	"name":"bit3/contao-doctrine-orm-timestampable",
	"description":"Doctrine ORM Timestampable Bridge for Contao CMS",
	"keywords":["contao", "doctrine", "orm", "timestampable"],
	"type":"contao-module",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Tristan Lins",
			"email":"tristan.lins@bit3.de",
			"homepage":"http://bit3.de",
			"role":"Developer"
		},
        {
            "name":"Dominik Tomasi",
            "email":"dominik.tomasi@gmail.com",
            "homepage":"https://github.com/ContaoBlackforest",
            "role":"Developer"
        },
        {
            "name":"Sven Baumann",
            "email":"baumannsv@gmail.com",
            "homepage":"https://github.com/ContaoBlackforest",
            "role":"Developer"
        }
	],
	"support":{
		"email":"info@bit3.de",
		"issues":"https://github.com/bit3/contao-doctrine-orm-timestampable/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/bit3/contao-doctrine-orm-timestampable"
	},
	"require":{
		"php":">=5.3",
		"contao/core": ">=2.11.11,<4-dev",
		"contao-community-alliance/composer-installer": "*",
		"bit3/contao-doctrine-orm": "~2.0",
        "doctrine/migrations": "1.0.0-alpha3",
		"gedmo/doctrine-extensions": "2.3.8"
	},
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
	"autoload":{
		"psr-0":{
			"Gedmo\\Timestampable\\Mapping\\Driver\\ContaoDca": "src/"
		},
        "psr-4":{
          "ContaoBlackForest\\": "src/",
          "ContaoBlackForest\\Tests\\": "tests/"
        },
        "files": ["src/namespace_backward_compat.php"]
	},
	"extra": {
		"contao": {
			"sources": {
				"contao": "system/modules/doctrine-orm-timestampable"
			}
		},
		"branch-alias": {
			"dev-master": "1.0.x-dev",
			"dev-develop": "2.0.x-dev"
		}
	}
}
